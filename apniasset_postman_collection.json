        {
            "name": "2. Authentication (OTP Flow)",
            "item": [
                {
                    "name": "User Registration",
                    "item": [
                        {
                            "name": "Send OTP",
                            "request": {
                                "method": "POST",
                                "url": "{{base_url}}/user/send-otp-registration",
                                "body": {
                                    "mode": "raw",
                                    "raw": "{\"phone\":\"9000011111\"}"
                                }
                            }
                        },
                        {
                            "name": "Verify OTP (Returns Registration Token)",
                            "request": {
                                "method": "POST",
                                "url": "{{base_url}}/user/verify-otp-registration",
                                "body": {
                                    "mode": "raw",
                                    "raw": "{\"phone\":\"9000011111\", \"otp\":\"123456\"}"
                                },
                                "description": "This now returns a `token` in the response data which should be used for the next step."
                            }
                        },
                        {
                            "name": "Complete Register (Only Name Required if Token provided)",
                            "request": {
                                "method": "POST",
                                "header": [
                                    {
                                        "key": "Authorization",
                                        "value": "Bearer {{registration_token}}",
                                        "description": "Token received from Verify OTP step"
                                    }
                                ],
                                "url": "{{base_url}}/user/register",
                                "body": {
                                    "mode": "raw",
                                    "raw": "{\"name\":\"Test User\"}"
                                },
                                "description": "Send only name if registration_token is provided. Phone, Email and Password are now optional."
                            }
                        }
                    ]
                },
                {
                    "name": "Merchant Registration",
                    "item": [
                        {
                            "name": "Send OTP",
                            "request": {
                                "method": "POST",
                                "url": "{{base_url}}/merchant/send-otp-registration",
