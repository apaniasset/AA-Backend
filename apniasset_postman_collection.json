                                    "mode": "raw",
                                    "raw": "{\"phone\":\"9000022222\"}"
                                }
                            }
                        },
                        {
                            "name": "Verify OTP",
                            "request": {
                                "method": "POST",
                                "url": "{{base_url}}/merchant/verify-otp-registration",
                                "body": {
                                    "mode": "raw",
                                    "raw": "{\"phone\":\"9000022222\", \"otp\":\"123456\"}"
                                }
                            }
                        },
                        {
